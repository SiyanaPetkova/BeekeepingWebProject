@model IEnumerable<PictureViewModel>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-F3uc1Xsnl9Cp90Fm1f4Fe9slkz5yTSW4WDe1Ifmvl3Qa6S++p82xwbQ1Ax4+xIQo" crossorigin="anonymous">
</head>
<body>
    <div class="container">
        <h1>Image Gallery</h1>
        <div class="row">
            @foreach (var imageUrl in Model)
            {
                <div class="col-md-4">
                    <div class="card mb-4">
                        <img src="@imageUrl" class="card-img-top" alt="Image">
                        <div class="card-body">
                            <h5 class="card-title">Image Title</h5>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/js/bootstrap.bundle.min.js" integrity="sha384-DlSsBfYJbhNwDRF3zk+vLd3bHHvO2fnrgiKv4+yJp0WSFA6kevT/I81kFnyqFgGS" crossorigin="anonymous"></script>
</body>
</html>